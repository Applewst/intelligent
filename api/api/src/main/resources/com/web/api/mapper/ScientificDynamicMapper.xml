<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.web.api.mapper.ScientificDynamicMapper">
    <select id="selectNewDynamics" resultType="com.web.api.pojo.ScientificDynamic">
        select *
        from scientific_dynamic
        order by id DESC
        limit #{num}
    </select>
    <select id="getAllDynamic" resultType="com.web.api.pojo.ScientificDynamic">
        select *
        from scientific_dynamic
    </select>
</mapper>